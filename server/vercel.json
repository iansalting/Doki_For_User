{
    "version": 2,
    "builds": [
        {"src": "server.js", "use": "vercel/node"}
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "MY_ENV_VAR": "value"
    }
}
